# Project Setup
EXECNAME =	test
SRCDIR =	src
OBJDIR =	obj
BINDIR =	bin

# Compiler
CC =	clang

# Compiler Flags
CFLAGS =	-std=c99 -pedantic -Wall -g

# Linker Flags
LDFLAGS =	

############################################################
DEPS=$(OBJDIR)/test.o $(OBJDIR)/xfs.o $(OBJDIR)/reiserfs.o 

all: $(DEPS)
	$(CC) -o $(BINDIR)/$(EXECNAME) $(DEPS) $(LDFLAGS)

$(OBJDIR)/%.o: $(SRCDIR)/%.c
	$(CC) $(CFLAGS) -c -o $@ $<

clean:
	find $(OBJDIR) -name "*.o" -print0 | xargs -0 rm -rf

cleaner: clean
	rm -f $(BINDIR)/$(EXECNAME)

