# Project Setup
EXECNAME =	test
SRCDIR =	src
OBJDIR =	obj
BINDIR =	bin

# Compiler
CC =	clang

# Compiler Flags
CFLAGS =	-std=c99 -pedantic -Wall -g -Iinclude -D_GNU_SOURCE 

# Linker Flags
LDFLAGS =	-Llib -lxfs -lext2fs -lreiserfs -lcom_err -ldal

############################################################
DEPS=$(OBJDIR)/test.o $(OBJDIR)/fsparser.o $(OBJDIR)/fs/ext.o $(OBJDIR)/fs/xfs.o $(OBJDIR)/fs/reiser.o

all: $(DEPS)
	$(CC) -o $(BINDIR)/$(EXECNAME) $(DEPS) $(LDFLAGS)

$(OBJDIR)/%.o: $(SRCDIR)/%.c
	$(CC) $(CFLAGS) -c -o $@ $<

clean:
	find $(OBJDIR) -name "*.o" -print0 | xargs -0 rm -rf

cleaner: clean
	rm -f $(BINDIR)/$(EXECNAME)

